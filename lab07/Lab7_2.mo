model Lab7_2
  // time [0:0.00001:0.02] 
  parameter Integer x0 = 8; // Начальное число знающих о товаре
  parameter Integer N = 1030; // максимальное число людей, которых может заинтересовать товар
  Real x(start=x0);
equation
  der(x) = (0.000083 + 0.83*x)*(N-x);
  // Пик распространения приходится на момент, когда x = 475, то есть половина от всех людей

end Lab7_2;
